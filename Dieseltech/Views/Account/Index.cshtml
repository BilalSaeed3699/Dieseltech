</head>
<body>
    @*<div class="se-pre-con"></div>*@

    <div class="main-wrapper account-wrapper">
        <div class="account-page">
            <div class="account-center">
                <div class="account-box">
                    <span style="color:red;" id="fademsg">@ViewBag.error</span>
                    @using (Html.BeginForm("Index", "Account", FormMethod.Post))
                    {
                        @Html.AntiForgeryToken()
                        <form class="form-signin">
                            <div class="account-logo">
                                @*<a href="#"><img src="~/assets/img/Logo_1.jpg" alt=""></a>*@
                                <img style="max-height:120px !important" src="~/assets/img/Logo_1.png" />

                                @*<a href="#"><img src="~/assets/img/Main_Logo.png" alt=""></a>*@

                            </div>
                            <div class="form-group">
                                <label> User Name</label>
                                <input type="text" required autofocus="" class="form-control" id="userLogin" name="userLogin">
                            </div>
                            <div class="form-group">
                                <label>Password</label>
                                <input type="password" required class="form-control" id="userPassword" name="userPassword">
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group text-left">
                                        <a href="@Url.Action("Index", "Privacy")" style="text-decoration:underline">Privacy Policy</a>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group text-right">
                                        <a href="@Url.Action("ForgetPassword", "Account")">Forgot your password?</a>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group text-center">
                                <button type="submit" style=" background-color: @Session["Color"] !important" ; class="btn btn-primary account-btn">Login</button>
                            </div>
                            <div class="text-center register-link">
                                Don’t have an account? <a href="@Url.Action("Register", "Account")">Register Now</a>
                            </div>


                        </form>
                    }
                </div>
            </div>
        </div>
    </div>
    <script src="~/assets/js/jquery-3.2.1.min.js"></script>
    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/app.js"></script>
</body>
